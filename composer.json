{
  "name": "georg/mysqlnd_ed25519",
  "description": "PECL extension for MySQL native driver ed25519 authentication plugin",
  "type": "php-ext",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Georg Richter",
      "email": "georg@php.net",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=8.0"
  },
  "php-ext": {
      "extension-name": "mysqlnd_ed25519",
      "priority": 30,
      "support-zts": true,
      "support-nts": true,
      "configure-options": [
        {
          "name": "with-mysqlnd-ed25519",
          "description": "Enable MariaDB ed25519 authentication support",
          "needs-value": false
        }
      ]
  }
}

